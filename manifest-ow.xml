<?xml version="1.0" encoding="utf-8"?>
<Modules xmlns="https://standaarden.overheid.nl/stop/DSO-PI10"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://standaarden.overheid.nl/stop/DSO-PI10/
  https://standaarden.overheid.nl/stop/DSO-PI10/manifest-ow.xsd">
  <!--
	Het domein element kan alleen omgevingswet zijn.
	Nodig om de verwerking in de keten beter te laten verlopen.
	-->
  <domein>omgevingswet</domein>
  <!-- Voor elke "imop:wordt" versie die in een besluit staat:	-->
  <RegelingVersie>
    <!--
		Verwijzing naar de versie van de regeling waar de modules bijhoren
		-->
    <FRBRwork>/akn/nl/act/pv25/2018/reg122</FRBRwork>
    <FRBRExpression>/akn/nl/act/pv25/2018/reg122/nld@2;2018-12-16</FRBRExpression>
    <!-- 
		Lijst met namen van bestanden die de CIM-OW data behorend bij
		de regelingversie bevatten. De bestanden moeten ook in
		manifest.xml voorkomen. Het objecttype moet zijn conform CIM-OW
		specificatie.
		-->
    <Bestand>
      <naam>owActiviteiten-Gemeentestad-1-template.xml</naam>
      <objecttype>Activiteit</objecttype>
    </Bestand>
    <Bestand>
      <naam>owFuncties-Gemeentestad-1-template.xml</naam>
      <objecttype>Functie</objecttype>
    </Bestand>
    <Bestand>
      <naam>owBeperkingengebied-Gemeentestad-1-template.xml</naam>
      <objecttype>Beperkingengebied</objecttype>
    </Bestand>
    <Bestand>
      <naam>owLocaties-Gemeentestad-2-template.xml</naam>
      <objecttype>Locatiegroep</objecttype>
      <objecttype>Gebied</objecttype>
    </Bestand>
    <Bestand>
      <naam>owLocaties-Gemeentestad-1-template.xml</naam>
      <objecttype>Locatiegroep</objecttype>
      <objecttype>Gebied</objecttype>
    </Bestand>
    <Bestand>
      <naam>owNormen-Gemeentestad-1-template.xml</naam>
      <objecttype>Norm</objecttype>
    </Bestand>
    <Bestand>
      <naam>owRegeltekst-Gemeentestad-1-template.xml</naam>
      <objecttype>Regeltekst</objecttype>
    </Bestand>
  </RegelingVersie>
</Modules>
